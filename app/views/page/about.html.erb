<% @panel_abouts.each do |panel_about| %>
<h4 class="flow-text">About Us</h4>

<div class="card">
    <div class="card-image waves-effect waves-block waves-light">
      <%= image_tag panel_about.image.url(:medium), class: "activator" %>
    </div>
    <div class="card-content">
      <span class="card-title activator grey-text text-darken-4"><%= panel_about.title %></span>
      <p class="text-justify"><%= panel_about.about %></p>
    </div>

  </div>
<blockquote>
  <p>
    <%= panel_about.blockquote %>
  </p>
</blockquote>
<% end %>