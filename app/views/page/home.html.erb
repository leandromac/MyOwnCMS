<% @panel_customs.each do |panel_custom| %>
<div class="carousel carousel-slider center" data-indicators="true">
    <a class="carousel-item" href="#one!"><img src="https://lorempixel.com/800/400/food/1">
    </a>
    <a class="carousel-item" href="#two!"><img src="https://lorempixel.com/800/400/food/2"></a>
    <a class="carousel-item" href="#three!"><img src="https://lorempixel.com/800/400/food/3"></a>
    <a class="carousel-item" href="#four!"><img src="https://lorempixel.com/800/400/food/4"></a>
  </div>

  <div class="col s12 m8 offset-m2 l6 offset-l3">
    <div class="card-panel grey lighten-5 z-depth-1">
      <div class="row valign-wrapper">
        <div class="col s2">
          <img src="http://lorempixel.com/400/400/cats/" alt="" class="circle responsive-img"> <!-- notice the "circle" class -->
        </div>
        <div class="col s10">
          <span class="black-text">
            <h4 class="flow-text" style="margin: 0px">Gatonildo Miow</h4>
            <%= panel_custom.about %>
          </span>
        </div>
      </div>
    </div>
  </div>

  <h4 class="flow-text">Our Services</h4>

  <div class="row">

    <% @panel_services.each do |panel_service| %>
      <div class="col s12 m3">
        <div class="card">
          <div class="card-image">
            <img src="<%= panel_service.image %>">
            <%= link_to panel_service, class: 'btn-floating halfway-fab waves-effect waves-light red' do %>
              <i class="material-icons">add</i>
            <% end %>
          </div>
          <div class="card-content">
            <p class="card-title"><%= panel_service.title %></p>
            <p><%= truncate(panel_service.description, length: 80) %></p>
          </div>
        </div>
      </div>
    <% end %>

  </div>

  <h4 class="flow-text">Our Clients</h4>
  <div class="col s12 m8 offset-m2 l6 offset-l3">
      <div class="row valign-wrapper">
        <div class="col s2">
          <img src="http://via.placeholder.com/400x400" alt="" class="circle responsive-img"> <!-- notice the "circle" class -->
        </div>
        <div class="col s2">
          <img src="http://via.placeholder.com/400x400" alt="" class="circle responsive-img"> <!-- notice the "circle" class -->
        </div>
        <div class="col s2">
          <img src="http://via.placeholder.com/400x400" alt="" class="circle responsive-img"> <!-- notice the "circle" class -->
        </div>
        <div class="col s2">
          <img src="http://via.placeholder.com/400x400" alt="" class="circle responsive-img"> <!-- notice the "circle" class -->
        </div>
        <div class="col s2">
          <img src="http://via.placeholder.com/400x400" alt="" class="circle responsive-img"> <!-- notice the "circle" class -->
        </div>
        <div class="col s2">
          <img src="http://via.placeholder.com/400x400" alt="" class="circle responsive-img"> <!-- notice the "circle" class -->
        </div>
      </div>
  </div>
<% end %>